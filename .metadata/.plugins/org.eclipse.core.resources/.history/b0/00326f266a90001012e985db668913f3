package pack;

public class Example 
{
	// method overloading the following code is 
	/*
	public int add(int x,int y)
	{
		return (x+y);
	}
	public void add(int a,int b,int c)
	{
		System.out.println("The addition of a,b,c is"+(a+b+c));
	}
	public static void main(String[] args) {
		Example ex = new Example();
		
		ex.add(10,20, 30);
		System.out.println("The add is "+ex.add(10,20));
	}
*/
	public int add(int a,int b)
	{
		return (a+b);
	}
}
public class Ex extends Ex
{
	public void add(int a,int b)
	{
		super.add(a, b);
		System.out.println("The addition of a,b is"+(a+b));
	}
	public static void main(String[] args) {
		Ex ex = new Ex();
		
		ex.add(10,20);
		System.out.println("The add is "+ex.add(10,20));
	}
}
