package java_programs;

import java.util.Scanner;

public class fourth1 
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter n value:.......");
		int n=sc.nextInt();
		int i,j;
		for(j=1;j<=n;j++)
		{
			for(i=1;i<=j;i++)
			{
				System.out.print(i);
			}
			System.out.println();		}
		
	}
}
